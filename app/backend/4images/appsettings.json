{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${DEFAULT_CONNECTION}",
    "AzureBlobStorage": "${AZURE_BLOB_STORAGE}"
  },
  "Jwt": {
    "SecretKey": "${SECRET_KEY}"
  },
  "Authentication": {
    "Google": {
      "ClientId": "${GOOGLE_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_CLIENT_SECRET}"
    }
  }
}
